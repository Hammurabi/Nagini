# Nagini Turing Complete Test
# Demonstrates: functions, arithmetic, booleans, if/else, while loops, recursion

def is_prime(n: int) -> int:
    """Check if a number is prime (returns 1 for true, 0 for false)"""
    if n <= 1:
        return 0
    if n <= 3:
        return 1
    if n % 2 == 0:
        return 0
    
    i = 3
    while i * i <= n:
        if n % i == 0:
            return 0
        i = i + 2
    return 1

def fibonacci(n: int) -> int:
    """Calculate fibonacci number using iteration"""
    if n <= 0:
        return 0
    if n == 1:
        return 1
    
    a = 0
    b = 1
    i = 2
    while i <= n:
        temp = a + b
        a = b
        b = temp
        i = i + 1
    return b

def gcd(a: int, b: int) -> int:
    """Calculate greatest common divisor using Euclidean algorithm"""
    while b != 0:
        temp = b
        b = a % b
        a = temp
    return a

def power(base: int, exp: int) -> int:
    """Calculate power using iterative multiplication"""
    result = 1
    i = 0
    while i < exp:
        result = result * base
        i = i + 1
    return result

