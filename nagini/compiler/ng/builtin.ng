@property(paradigm='object')
class Point:
    x: float
    y: float
    
    def __init__(self, x: float, y: float):
        self.x = x
        self.y = y
    
    def __str__(self) -> str:
        return f"Point({self.x:.2f}, {self.y:.2f})"

def distance(p1, p2) -> float:
    dx = p2.x - p1.x
    dy = p2.y - p1.y
    return (dx**2 + dy**2) ** 0.5
